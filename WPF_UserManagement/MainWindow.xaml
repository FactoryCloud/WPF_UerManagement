<Window x:Class="WPF_UserManagement.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_UserManagement"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="5*" />
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="5*"/>
        </Grid.RowDefinitions>
        <DockPanel Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3">
            <Button>Top Side</Button>
        </DockPanel>
        <DockPanel Grid.Column="0" Grid.Row="1">
            <Frame Name="frame" Source="Page1.xaml" />
        </DockPanel>
        <DockPanel Grid.Column="1" Grid.Row="1">
            <Button Click="Button_Click">Event</Button>
        </DockPanel>
        <DockPanel Grid.Column="2" Grid.Row="1">
            <Frame Name="frame2"/>
        </DockPanel>
        

    </Grid>
</Window>
